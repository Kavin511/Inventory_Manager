<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.are.vehiclemanager.ui.filler.fillerFragment"
        android:label="Oil Filters Inspection Report"
        tools:layout="@layout/fragment_filler" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.are.vehiclemanager.ui.equipment.equipmentFragment"
        android:label="Equipment Inspection Report"
        tools:layout="@layout/fragment_equipment" />
    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.are.vehiclemanager.ui.account.accountFragment"
        android:label="Account"
        tools:layout="@layout/fragment_account" />
    <fragment
        android:id="@+id/signout"
        android:name="com.are.vehiclemanager.signout"
        android:label="Sign Out"
        tools:layout="@layout/fragment_signout" />

</navigation>